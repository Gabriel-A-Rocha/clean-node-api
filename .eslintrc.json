{
  "extends": [
    "standard-with-typescript",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "@typescript-eslint/strict-boolean-expressions": "off"
  }
}
